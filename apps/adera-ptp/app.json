{
  "expo": {
    "name": "adera-ptp",
    "slug": "adera-ptp",
    "version": "1.0.0",
    "entryPoint": "index.js",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#2E7D32"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "never",
      "fallbackToCacheTimeout": 0
    },
    "scheme": "adera",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adera.app"
    },
    "android": {
      "backgroundColor": "#2E7D32",
      "package": "com.adera.app"
    },
    "web": {
      "bundler": "metro"
    }
  }
}
